<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.temper" name="temper" version="1.1" provider-name="pyro"> 
	<requires>
		<import addon="xbmc.python" version="2.19.0" />
		<import addon="script.module.universalscrapers" version="1.0.2" />
        <import addon="script.module.python.koding.aio" version="0.0.1"/>
	</requires>
	<extension point="xbmc.python.pluginsource" library="default.py">
		<provides>audio video</provides>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary lang="en">Music</summary> 
		<description lang="en">Music to relax too. All videos are uploaded by their creators on YouTube. We do not host or own any content</description>
		<platform>all</platform>
		<news></news>
		<genre>music</genre>
		<video_guide></video_guide>
		<video_preview></video_preview>
	</extension>
</addon>
